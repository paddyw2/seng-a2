CREATE(5, 10, 25, 100; 3; 10; 10; 10)
CREATE(5, 10, 25, 100; 2; 10; 10; 10)
CONFIGURE([1] "Diet", 200; "water", 250)
CONFIGURE([0] "Diet", 200; "water", 250; "stuff", 205)
COIN_LOAD([0] 3; 100, 1)
COIN_LOAD([0] 3; 100, 1)
COIN_LOAD([1] 2; 25, 5)
POP_LOAD([1] 0; "Diet", 1)
POP_LOAD([0] 0; "RandomPop", 1)
POP_LOAD([0] 0; "Diet", 1)
POP_LOAD([0] 1; "water", 1)
POP_LOAD([0] 2; "stuff", 1)
INSERT([0] 100)
INSERT([0] 100)
INSERT([0] 100)
INSERT([1] 100)
INSERT([1] 100)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(100, "RandomPop")
UNLOAD([0])
CHECK_TEARDOWN(400; 0; "Diet", "stuff", "water")
PRESS([1] 0)
EXTRACT([1])
CHECK_DELIVERY(0, "Diet")
UNLOAD([1])
CHECK_TEARDOWN(325; 0)
